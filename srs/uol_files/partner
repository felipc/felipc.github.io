btdata = ["//tracker.bt.uol.com.br/bt-track-uol.html?value=%3B212%3B"]; for(i=0;i<btdata.length;i++){ var id = "tracker-uol-partner-" + (i+1); if(document.getElementById(id)) { continue; } var urlCookie = btdata[i]; var type = urlCookie.indexOf('tailtarget') !== -1 ? 'img' : 'iframe'; ifrm = document.createElement(type); ifrm.setAttribute("id", id); ifrm.setAttribute("src", urlCookie); ifrm.style.width = "0px"; ifrm.style.height = "0px"; ifrm.style.border = "0px"; ifrm.style.display = "none"; ifrm.setAttribute("frameborder", "0"); if(document.body != null) { document.body.appendChild(ifrm); } }